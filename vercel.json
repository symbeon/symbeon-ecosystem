{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/symbeon-showcase/dist",
  "devCommand": "npm run dev",
  "framework": "vite",
  "crons": [],
  "regions": ["iad1"],
  "env": [],
  "build": {
    "env": ["NODE_ENV"]
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/:path*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
