{
  "buildCommand": "cd frontend/symbeon-showcase && npm run build",
  "outputDirectory": "frontend/symbeon-showcase/dist",
  "devCommand": "cd frontend/symbeon-showcase && npm run dev",
  "installCommand": "cd frontend/symbeon-showcase && npm install",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
